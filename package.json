{
  "name": "playground",
  "version": "0.0.1",
  "description": "playground",
  "main": "index.js",
  "scripts": {
    "setup": "npm install -g webpack && npm install",
    "dev": "cross-env NODE_ENV=development webpack-dev-server --config ./webpack.config.js --progress --colors --open --hot",
    "watch": "webpack --config ./webpack.config.js --progress --colors --watch",
    "prebuild": "eslint lib test",
    "build": "cross-env NODE_ENV=production webpack --config ./webpack.config.js --progress --colors --hide-modules && cp -rf public/* dist/",
    "test": "karma start",
    "prepublish": "npm test"
  },
  "repository": {
    "url": "https://github.com/egaoneko/playground.js.git",
    "type": "git"
  },
  "author": "Donghyun Seo",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/egaoneko/playground.js/issues"
  },
  "homepage": "hhttps://github.com/egaoneko/playground.js#readme",
  "devDependencies": {
    "babel-core": "^6.21.0",
    "babel-loader": "^6.2.10",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-latest": "^6.22.0",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.24.1",
    "bdd-lazy-var": "^1.3.0",
    "chai": "^3.5.0",
    "cross-env": "^3.2.3",
    "css-loader": "^0.26.1",
    "eslint": "^3.13.1",
    "eslint-loader": "^1.6.1",
    "eslint-plugin-react": "^7.0.0",
    "extract-text-webpack-plugin": "^2.1.2",
    "imports-loader": "^0.7.0",
    "istanbul": "^0.4.5",
    "karma": "^1.6.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-coverage": "^1.1.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-firefox-launcher": "^1.0.1",
    "karma-mocha": "^1.3.0",
    "karma-webpack": "^2.0.3",
    "mocha": "^3.2.0",
    "node-sass": "^4.5.3",
    "sass-loader": "^4.1.1",
    "sinon": "^1.17.7",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1",
    "webpack-dev-server": "^2.2.1"
  },
  "dependencies": {
